{
	"name": "choreapp-api",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"test": "jest --no-cache --coverage",
		"start": "yarn generate && serverless offline --stage local",
		"deploy": "yarn generate && bash deploy.sh env=dev",
		"i": "bash install.sh",
		"latest": "yarn upgrade-interactive --latest",
		"generate": "graphql-codegen --config codegen.yml"
	},
	"dependencies": {
		"@apollo/gateway": "^0.11.6",
		"apollo-server": "^2.9.15",
		"apollo-server-lambda": "^2.9.15",
		"aws-lambda": "^0.1.2",
		"graphql": "^14.5.8",
		"graphql-tag": "^2.10.1"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^1.11.2",
		"@graphql-codegen/typescript": "^1.11.2",
		"@graphql-codegen/typescript-resolvers": "^1.11.2",
		"@types/aws-lambda": "^8.10.31",
		"@types/graphql": "^14.2.0",
		"@types/node": "^13.1.2",
		"aws-sdk": "^2.596.0",
		"concurrently": "^5.0.2",
		"serverless": "^1.60.4",
		"serverless-domain-manager": "^3.3.0",
		"serverless-dotenv-plugin": "^2.1.1",
		"serverless-offline": "^5.12.1",
		"serverless-webpack": "^5.3.0",
		"ts-loader": "^6.2.1",
		"typescript": "^3.7.4",
		"webpack": "^4.41.5",
		"webpack-graphql-loader": "^1.0.2",
		"webpack-node-externals": "^1.7.2"
	}
}
