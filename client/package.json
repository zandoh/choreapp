{
  "name": "choreapp-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/core": "^0.4.1",
    "@emotion/core": "^10.0.17",
    "@emotion/styled": "^10.0.17",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.3.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.2",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "@types/react-redux": "^7.1.2",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^5.1.3",
    "amazon-cognito-identity-js": "^3.0.15",
    "aws-sdk": "^2.520.0",
    "aws-sdk-mock": "^4.5.0",
    "emotion-theming": "^10.0.17",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "formik": "^2.0.6",
    "jest-styled-components": "^6.3.4",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.2.0",
    "react-test-renderer": "^16.12.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "serverless-finch": "^2.4.2",
    "ts-jest": "^24.1.0",
    "typescript": "3.7.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false",
    "test:watch": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "deploy": "bash deploy.sh",
    "serve": "serve -s build",
    "upgrade:latest": "yarn upgrade --interactive --latest"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/containers/**/*.tsx",
      "src/components/**/*.tsx",
      "src/store/**/*.ts",
      "src/services/**/*.ts",
      "!/node_modules/",
      "!src/*.ts",
      "!src/*.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": -10
      }
    }
  },
  "devDependencies": {
    "@fortawesome/fontawesome-pro": "^5.11.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/pro-light-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/redux-mock-store": "^1.0.1",
    "redux-mock-store": "^1.5.3"
  }
}
